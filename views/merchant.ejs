<%- include("partials/header.ejs"); -%>


<div class="merchantDisplay">


  <h1>Merchant</h1>
  <p> <%=merchantMessage%> </p>
  <% if(foundPartys.length!=0){ %>
  <% foundPartys.forEach(function(party){ %>
  <%   var fullPartyInfo = party.name +"| " + party.number + "| " + party.party; %>

  <form action="/delete" method="post">
    <div class="party">

      <input type="checkbox" name ="checkbox"  value="<%=party._id%>"  onChange="this.form.submit()">

        <%=fullPartyInfo%>

    </div>

  </form>

  <% }) } %>






</div>







<%- include("partials/footer"); -%>
