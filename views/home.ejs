<%- include("partials/header.ejs"); -%>


<div class ="mainImage">
  <h1>Welcome</h1>
  <img  src="/images/main.png" alt="logo">
  <p> <%=startingContent%> </p>

 <form class="" action="/" method="post">
  <label for="name">Name</label>
  <input type="text" name="postName" placeholder="Enter your name" name="Name" >

  <label for="number">Number</label>
  <input type="number" name="postNumber" placeholder="Enter your number" name="Number" >

  <label for="Party">Party size</label>
